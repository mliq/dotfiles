alias g="git"
# ------------------------------------------------------------------------------
# Reload current shell
# ------------------------------------------------------------------------------
alias rl="source ~/.zshrc"

# ------------------------------------------------------------------------------
# Run tmux to make it work with colors
# ------------------------------------------------------------------------------
alias tmux="TERM=screen-256color-bce tmux"

# ------------------------------------------------------------------------------
# Setup NVM (Might not need this)
# ------------------------------------------------------------------------------
export NVM_DIR="$HOME/.nvm"
. "$(brew --prefix nvm)/nvm.sh"
