#function run
run() {
    number=$1
    shift
    for i in `seq $number`; do
      $@
    done
}
alias be="bundle exec"
alias f="find"
alias g="git"
alias h="cd ~/hack"
alias notes="vim ~/.notes"
alias today="cat ~/.notes/today"
alias o="open"
alias rl="source ~/.zshrc"
alias tmux="TERM=screen-256color-bce tmux"
alias n="npm"

alias fa="find . | ag"
